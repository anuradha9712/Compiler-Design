# 
# ========== Q9.l file==================

%option noyywrap
%{
  #include "y.tab.h"
%}
%%
[0-9]+ return digit;
[a-zA-Z][a-zA-Z]* return id;
[+-//*] return op;
.|[\n] return 0;
%%

========== Q9.y file=====================
%{
 #include "lex.yy.c"
 int valid = 1;
%}
%token digit id op
%left op
%%
start: exp;
exp: exp op exp
   | id
   | digit
;
%%
int yyerror()
{ valid =0;
  printf("invalid expression);
  return 0;
}
int main()
{ printf("enter expression");
yyparse();
if(valid) printf("valid");
}
